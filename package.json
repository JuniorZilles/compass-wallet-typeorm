{
  "name": "compass-wallet",
  "version": "1.0.1",
  "description": "",
  "author": "Junior Zilles",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "typeorm migration:run",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "set NODE_ENV=test&jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:watch": "set NODE_ENV=test&jest --config ./test/jest-e2e.json --watch",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "8.2.5",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.2.5",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "8.2.5",
    "@nestjs/swagger": "5.1.5",
    "@nestjs/typeorm": "8.0.2",
    "axios": "0.24.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "moment": "2.29.1",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.2",
    "swagger-ui-express": "4.3.0",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.8",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.5",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "chance": "1.1.8",
    "eslint": "8.6.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.1",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4",
    "uuid": "8.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
